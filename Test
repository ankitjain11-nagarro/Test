SELECT whatsapp_template.id, whatsapp_template.sequence, whatsapp_template.content, whatsapp_template.type, whatsapp_template.url_shorten, whatsapp_template.meta_template_name, whatsapp_template.sender_id, whatsapp_template.media_type, whatsapp_template.media_url, whatsapp_template.media_file_name, whatsapp_template.template_id, whatsapp_template.form_code, whatsapp_template.form_name, whatsapp_template.media_channel, whatsapp_template.media_meta_id, whatsapp_template.footer_content, whatsapp_template.meta_template_id, whatsapp_template.media_extension, whatsapp_template.bsp, whatsapp_template.bsp_sub_type FROM whatsapp_template WHERE whatsapp_template.template_id = $1
