[SELECT distinct s.campaign_code,s.segment_code,s.segmentType,s.orchestration_type, CONCAT(cs.start_date, ' ', cs.start_time) AS execution_start_date, cs.status, s.template_code, cs.frequency, cs.scheduler_code,cs.scheduler_detail,  c.name AS campaign_name, c.end_date AS campaign_end_date, c.start_date AS campaign_start_date, s.segment_name, cs.modified_date, s.mode_of_execution, s.created_by, t.name AS template_name, s.record_count AS total_records, s.valid_count AS passed_records, csc.waba_number, csc.email_sender_id, t.type AS channel, wt.bsp, wt.bsp_sub_type,  csc.channel_name, csc.channel_sequence,csc.waba_code, csc.link_wait_time,csc.channel_wait_time,csc.waba_code, csa.last_executed_on,s.segment_code, s.form_code, cs.frequency,cs.cron_exp_desc, ra.number as agent_number, ecm.name AS email_sender_name FROM campaign_scheduler cs  INNER JOIN segments s ON cs.segment_code = s.segment_code  INNER JOIN campaigns c ON c.code = s.campaign_code  INNER JOIN templates t ON s.template_code = t.code  LEFT JOIN whatsapp_template wt on t.id =wt.template_id  LEFT JOIN (SELECT DISTINCT rt.template_id, rt.rcs_agent_id FROM rcs_template rt) rt ON t.id = rt.template_id  LEFT JOIN rcs_agent ra ON rt.rcs_agent_id = ra.id  LEFT JOIN scheduler_channel_config csc ON cs.id = csc.scheduler_id LEFT JOIN email_configuration_master ecm ON csc.email_sender_id = ecm.sender_id LEFT JOIN (SELECT scheduler_code, MAX(modified_date) AS last_executed_on FROM campaign_scheduler_audit WHERE status = 'Executed' GROUP BY scheduler_code) csa ON  cs.scheduler_code = csa.scheduler_code WHERE cs.scheduler_code = $1
