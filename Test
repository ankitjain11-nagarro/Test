-- Create schema if not exists
CREATE SCHEMA IF NOT EXISTS customer_data;

CREATE TABLE customer_data.consent (
    created_date timestamp with time zone,
    icp_id bigint NOT NULL,
    mobile_no bigint NOT NULL,
    last_modified_date timestamp with time zone,
    consent_status boolean NOT NULL,
    cust_name character varying(100),
    email_id character varying(255),
    email_notify boolean,
    face_id boolean,
    push_notify boolean,
    sms_notify boolean,
    wa_notify boolean,
    account_status character varying(150),
    car_profile_add_status boolean,
    profile_created_status boolean,
    svoc_id bigint,
    created_by character varying(255),
    last_modified_by character varying(255),
    delete_request character(1)
);


