private Mono<TemplateBundle> validateRegularTemplateBundle(TemplateBundle templateBundle, boolean isOrchestrateMode, List<WhatsAppCTA> whatsAppList) {
		if (whatsAppList.size() > 1 && Boolean.FALSE.equals(isOrchestrateMode)) {
